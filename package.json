{
  "name": "@jupiterone/graph-azure",
  "version": "1.1.0",
  "description": "A graph conversion tool for https://azure.microsoft.com/.",
  "main": "dist/index.js",
  "repository": "https://github.com/JupiterOne/graph-azure",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "LICENSE",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "ts-node --require dotenv/config tools/execute.ts | bunyan",
    "start:graph": "docker run -d --rm --name jupiterone-local-graph -p 8182:8182 tinkerpop/gremlin-server:3.3",
    "stop:graph": "docker stop jupiterone-local-graph",
    "prebuild": "yarn test",
    "build": "rm -rf dist/* && yarn build:types && yarn build:src && yarn build:docs",
    "build:src": "babel src --extensions '.ts' --out-dir dist",
    "build:docs": "ts-node tools/docs.ts",
    "build:types": "tsc --project tsconfig.dist.json --declaration --emitDeclarationOnly",
    "build:docs:model": "ts-node tools/entities-relationships-table-md.ts",
    "lint": "yarn lint:es && yarn lint:md",
    "lint:md": "remark .",
    "lint:es": "eslint . --ext .ts --ignore-pattern dist",
    "format": "yarn prettier --write '**/*.{ts,js,json,md}'",
    "test": "jest"
  },
  "dependencies": {
    "@azure/arm-compute": "^11.0.0",
    "@azure/ms-rest-nodeauth": "^2.0.5",
    "@jupiterone/jupiter-managed-integration-sdk": "^24.2.0",
    "@microsoft/microsoft-graph-client": "^2.0.0",
    "cross-fetch": "^3.0.4",
    "lodash.map": "^4.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@microsoft/microsoft-graph-types": "^1.10.0",
    "@pollyjs/adapter-node-http": "^2.6.2",
    "@pollyjs/core": "^2.6.2",
    "@pollyjs/persister-fs": "^2.6.2",
    "@types/bunyan": "^1.8.5",
    "@types/fs-extra": "^5.0.5",
    "@types/har-format": "^1.2.4",
    "@types/jest": "^24.0.0",
    "@types/lodash.map": "^4.6.13",
    "@types/nock": "^9.3.1",
    "@types/node": "^10.12.20",
    "@types/pollyjs__adapter-node-http": "^2.0.0",
    "@types/pollyjs__core": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "bunyan": "^1.8.12",
    "dotenv": "^7.0.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "fs-extra": "^7.0.1",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "jest-haste-map": "^24.0.0",
    "jest-resolve": "^24.1.0",
    "lint-staged": "^8.1.3",
    "nock": "^10.0.6",
    "prettier": "1.16.4",
    "remark-cli": "^6.0.1",
    "remark-lint": "^6.0.4",
    "remark-preset-lint-consistent": "^2.0.2",
    "remark-preset-lint-markdown-style-guide": "^2.1.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  }
}

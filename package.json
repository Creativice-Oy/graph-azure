{
  "name": "@jupiterone/graph-azure",
  "version": "3.15.4",
  "description": "A graph conversion tool for https://azure.microsoft.com/.",
  "main": "dist/index.js",
  "repository": "https://github.com/JupiterOne/graph-azure",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "LICENSE",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "ts-node --require dotenv/config tools/execute.ts | bunyan",
    "start:containers": "docker-compose up -d",
    "stop:containers": "docker-compose down",
    "restart:containers": "docker-compose restart",
    "prepublishOnly": "yarn build",
    "prebuild": "yarn test",
    "build": "rm -rf dist/* && yarn build:types && yarn build:src && yarn build:docs",
    "build:src": "babel src --extensions '.ts' --out-dir dist",
    "build:docs": "ts-node tools/docs.ts",
    "build:types": "tsc --project tsconfig.dist.json --declaration --emitDeclarationOnly",
    "build:docs:model": "ts-node tools/entities-relationships-table-md.ts",
    "lint": "yarn lint:es && yarn lint:md",
    "lint:md": "remark .",
    "lint:es": "eslint . --ext .ts --ignore-pattern dist",
    "format": "yarn prettier --write '**/*.{ts,js,json,md}'",
    "test": "jest",
    "tf:init": "docker run --env-file terraform/.env -i -t -v `pwd`/terraform:/azure -w /azure hashicorp/terraform:light init",
    "tf:plan": "docker run --env-file terraform/.env -i -t -v `pwd`/terraform:/azure -w /azure hashicorp/terraform:light plan",
    "tf:apply": "docker run --env-file terraform/.env -i -t -v `pwd`/terraform:/azure -w /azure hashicorp/terraform:light apply",
    "tf:destroy": "docker run --env-file terraform/.env -i -t -v `pwd`/terraform:/azure -w /azure hashicorp/terraform:light destroy"
  },
  "dependencies": {
    "@azure/arm-compute": "^14.0.0",
    "@azure/arm-cosmosdb": "^7.0.0",
    "@azure/arm-keyvault": "^1.2.1",
    "@azure/arm-mariadb": "^1.4.0",
    "@azure/arm-mysql": "^3.3.0",
    "@azure/arm-network": "^20.0.0",
    "@azure/arm-postgresql": "^4.4.0",
    "@azure/arm-sql": "^7.0.0",
    "@azure/arm-storage": "^13.1.0",
    "@azure/identity": "^1.0.2",
    "@azure/keyvault-keys": "^4.0.2",
    "@azure/ms-rest-nodeauth": "^2.0.5",
    "@jupiterone/jupiter-managed-integration-sdk": "33.7.5",
    "@lifeomic/attempt": "^3.0.0",
    "@microsoft/microsoft-graph-client": "^2.0.0",
    "cross-fetch": "^3.0.4",
    "lodash.foreach": "^4.5.0",
    "lodash.map": "^4.6.0",
    "lodash.snakecase": "^4.1.1"
  },
  "resolutions": {
    "@azure/ms-rest-azure-js": "^2.0.0",
    "@azure/ms-rest-js": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@microsoft/microsoft-graph-types": "^1.10.0",
    "@pollyjs/adapter-node-http": "^2.6.2",
    "@pollyjs/core": "^2.6.2",
    "@pollyjs/persister-fs": "^2.6.2",
    "@types/bunyan": "^1.8.5",
    "@types/fs-extra": "^5.0.5",
    "@types/gremlin": "^3.4.2",
    "@types/har-format": "^1.2.4",
    "@types/jest": "^24.0.0",
    "@types/lodash.foreach": "^4.5.6",
    "@types/lodash.map": "^4.6.13",
    "@types/lodash.snakecase": "^4.1.6",
    "@types/nock": "^9.3.1",
    "@types/node": "^10.12.20",
    "@types/pollyjs__adapter-node-http": "^2.0.0",
    "@types/pollyjs__core": "^2.6.0",
    "@types/vfile-message": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "aws-sdk": "^2.610.0",
    "bunyan": "^1.8.12",
    "dotenv": "^7.0.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "fs-extra": "^7.0.1",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "jest-haste-map": "^24.0.0",
    "jest-resolve": "^24.1.0",
    "lint-staged": "^8.1.3",
    "nock": "^10.0.6",
    "prettier": "^2.0.2",
    "remark-cli": "^6.0.1",
    "remark-lint": "^6.0.4",
    "remark-preset-lint-consistent": "^2.0.2",
    "remark-preset-lint-markdown-style-guide": "^2.1.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.8.3"
  }
}
